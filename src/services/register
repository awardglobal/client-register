interface IndividualClientRegistration {
  email: string;
  firstName: string; 
  lastName: string;
  phone: string;
  purpose: string;
  // 如果purpose为other 则需要提供purposeOther
  otherPurpose?: string;
  saleId?: string;
  document1Front: File;
  document1Back: File;
  document2Front: File;
  document2Back: File;
  signature: File;
  facePic?: File;

  gender: number; // 0 male 1 femal 2 other
  dob: string; // format 1990-01-01    
  occupation: string;
  employerName: string;
  annualIncome: string;
  sourceOfIncome: string;

  address: string;
  suburb: string;
  state: string;
  country: string;
  postcode: string;

  beneficiray: IndividualClientBeneficiary;
}

interface IndividualClientBeneficiary {
  beneficiaryType: number; // 0 自己 1 他人
  bankName: string;
  branchName: string;
  accountName: string;
  accountNumber: string;
  bsb_swiftCode: string;

  trustAccount?: boolean;
  companyName?: string;
  companyAddress?: string;
  companyABN?: string;

  address?: string;
  suburb?: string;
  state?: string;
  postcode?: string;
  country?: string;

  firstName?: string;
  lastName?: string;
  dob?: string;
  phone?: string;
  relation?: string;
  occupation?: string;

  purpose?: string; //?more

  documentFront?: File;
  documentBack?: File;
}
